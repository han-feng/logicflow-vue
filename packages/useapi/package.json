{
  "name": "logicflow-useapi",
  "version": "0.4.0",
  "description": "Logic Flow Vue use API",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/logicflow-useapi.umd.cjs",
  "module": "dist/logicflow-useapi.js",
  "exports": {
    ".": {
      "import": "./dist/logicflow-useapi.js",
      "require": "./dist/logicflow-useapi.umd.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/han-feng/logicflow-vue/"
  },
  "keywords": [
    "LogicFlow",
    "Vue 3",
    "Use API",
    "Composition API"
  ],
  "scripts": {
    "all": "pnpm clean && pnpm build",
    "build": "vue-tsc --noEmit && vite build",
    "clean": "rimraf dist",
    "preinstall": "npx only-allow pnpm"
  },
  "peerDependencies": {
    "@logicflow/core": ">=1.1.10",
    "lodash-es": ">=4.10.0",
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@logicflow/core": "^1.1.26",
    "lodash-es": "^4.17.21",
    "resize-detector": "^0.3.0",
    "vue": "^3.2.38"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.14.184",
    "@types/node": "^18.7.14",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-visualizer": "^5.8.0",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.4.1",
    "vue-tsc": "^0.40.5"
  }
}
