{
  "name": "logicflow-useapi",
  "type": "module",
  "version": "0.6.0",
  "description": "Logic Flow Vue use API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/han-feng/logicflow-vue/"
  },
  "keywords": [
    "LogicFlow",
    "Vue 3",
    "Use API",
    "Composition API"
  ],
  "exports": {
    ".": {
      "require": "./dist/logicflow-useapi.umd.cjs",
      "import": "./dist/logicflow-useapi.js"
    }
  },
  "main": "dist/logicflow-useapi.umd.cjs",
  "module": "dist/logicflow-useapi.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "all": "pnpm clean && pnpm build",
    "build": "vue-tsc --noEmit && vite build",
    "clean": "rimraf dist",
    "preinstall": "npx only-allow pnpm"
  },
  "peerDependencies": {
    "@logicflow/core": ">=1.1.10",
    "lodash-es": ">=4.10.0",
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@logicflow/core": "^1.2.1",
    "lodash-es": "^4.17.21",
    "resize-detector": "^0.3.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0",
    "vue-tsc": "^1.2.0"
  }
