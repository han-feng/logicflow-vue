{
  "name": "logicflow-vue-demo",
  "version": "0.4.0",
  "private": true,
  "description": "Logic Flow Vue Demo",
  "scripts": {
    "all": "pnpm clean && pnpm build",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && node script/crlf.js",
    "preview": "vite preview",
    "cy:run": "cypress install && cypress run",
    "test": "pnpm test:e2e",
    "test:e2e": "start-server-and-test 'vite preview --open false' http://127.0.0.1:4173/ 'pnpm cy:run'",
    "clean": "rimraf dist",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@logicflow/core": "^1.1.22",
    "@logicflow/extension": "^1.1.22",
    "ant-design-vue": "^3.2.10",
    "highlight.js": "^11.6.0",
    "ids": "^1.0.0",
    "lodash": "^4.17.21",
    "logicflow-useapi": "workspace:^",
    "resize-detector": "^0.3.0",
    "splitpanes": "^3.1.1",
    "vue": "^3.2.37"
  },
  "optionalDependencies": {
    "cypress": "^10.3.1",
    "jimp": "^0.16.1",
    "start-server-and-test": "^1.14.0"
  },
  "devDependencies": {
    "@types/node": "^18.6.1",
    "@types/splitpanes": "^2.2.1",
    "@vitejs/plugin-vue": "^3.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "typescript": "^4.7.4",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.3",
    "vue-tsc": "^0.39.0"
  }
}
